{
  "name": "qr-sign-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "gcloud app deploy --verbosity=info --project qr-sign-site"
  },
  "dependencies": {
    "@shopify/draggable": "^1.0.0-beta.8",
    "core-js": "^3.6.4",
    "html2canvas": "^1.0.0-rc.5",
    "interactjs": "^1.9.17",
    "jspdf": "^1.5.3",
    "qrcode": "^1.4.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-useless-escape": "off",
      "no-unused-vars": "off"
    }
  }
}
